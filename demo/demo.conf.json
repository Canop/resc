{
	"watchers": [
		{
			"redis": {
				"url": "redis://127.0.0.1/"
			},
			"input_queue": "global/done",
			"taken_queue": "global/taken",
			"rules": [
				{
					"name": "TRT computation on data acquisition",
					"on": {
						"done": "^acq/(?P<process_id>\\d+)/(?P<product_id>\\d+)$"
					},
					"todo": {
						"task": "trt/${process_id}/${product_id}",
						"queue": "trt/${process_id}/todo",
						"set": "${product_id}/todo"
					}
				}
			]
		}
	]
}
